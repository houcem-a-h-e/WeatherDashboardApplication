    "esModuleInterop": true,
    "baseUrl": "./",
    "resolveJsonModule": true,
    "paths": {
      "*": ["src/assets/*"]
    },
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
